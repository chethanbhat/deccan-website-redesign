extends partials/head.pug

block title 
        title Storage Solutions

block content
    .container
        .card.purple-card
            .card-image
                img(src="images/cover/cabinets.png", class="category-page-img", alt="Liberty Cabinets, Storage Solutions, Deccan Plast Industries")
            .card-content.center-align
                h2.category-title.center-align Storage Solutions
                h3.product-title.center-align Liberty Cabinets Big 
                p.category-description  
                strong Liberty Cabinets Big 
                | is Rattan finished and wooden finished  cabinets for houses and commercial establishments 
                p.category-description 
                strong Recommended 
                |for Household, Office, Kitchens.
                p.category-description 
                strong Dimensions: 
                |(H) 1500 mm (W) 710 mm (D) 440 mm
                .row.center-align
                    .col.s12.m4
                        .card.product-card.z-depth-2
                            .card-image
                                img(src="images/products/furniture/liberty_cabinets_big_dbr_rattan.png", alt="Liberty Cabinets, Storage Solutions, Cupboard, Deccan Plast Industries")
                            .card-content.center-align.pink-card
                                span.card-title Liberty Big DBR
                                p.texture-desc (Rattan Finish)
                                p.price MRP: ₹ 7500/-
                                .right-align
                                    a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Liberty Big Rattan")
                                        i.material-icons shopping_cart 
                    .col.s12.m4
                        .card.product-card.z-depth-2
                            .card-image
                                img(src="images/products/furniture/liberty_cabinets_big_dbr_wooden.png", alt="Liberty Cabinets, Storage Solutions, Cupboard, Deccan Plast Industries")
                            .card-content.center-align.pink-card
                                span.card-title Liberty Big DBR
                                p.texture-desc (Wooden Finish)
                                p.price MRP: ₹ 7500/-
                                .right-align
                                    a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Liberty Big DBR Wooden")
                                        i.material-icons shopping_cart

                    .col.s12.m4
                        .card.product-card.z-depth-2
                            .card-image
                                img(src="images/products/furniture/liberty_cabinets_big_dbr_tco.png", alt="Liberty Cabinets, Storage Solutions, Cupboard, Deccan Plast Industries")
                            .card-content.center-align.pink-card
                                span.card-title Liberty Big TCO/DBR
                                p.texture-desc (Wooden Finish)
                                p.price MRP: ₹ 7500/-
                                .right-align
                                    a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Liberty Big TCO-DBR Wooden")
                                        i.material-icons shopping_cart   
                                                                        
                .product-btns                                         
                    a(href="https://www.youtube.com/watch?v=wCR3INyfND0").btn.btn-large.waves-effect.waves-light.red.white-text Assembly Instructions    
                    a(href="/pdf/liberty_big_ai.pdf",target="_blank").btn.btn-large.waves-effect.waves-light.red.white-text Download PDF                                                                            
                h3.product-title.center-align Liberty Cabinets Small 
                p.category-description  
                strong Liberty Cabinets Small
                | Rattan finished and wooden finished  cabinets for houses and commercial establishments 
                p.category-description 
                strong Recommended 
                |for Household, Office, Kitchens.
                p.category-description 
                strong Dimensions: 
                |(H) 790 mm (W) 710 mm (D) 440 mm
                .row.center-align
                    .col.s12.m4.offset-m4
                        .card.product-card.z-depth-2
                            .card-image.center-align
                                img(src="images/products/furniture/liberty_cabinets_small.png", alt="Liberty Cabinets, Storage Solutions, Cupboard, Deccan Plast Industries")
                            .card-content.center-align.pink-card
                                span.card-title Liberty Small
                                p.texture-desc (Wooden Finish)
                                p.price MRP: ₹ 4425/-
                                .right-align
                                    a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Liberty Small")
                                        i.material-icons shopping_cart 
                .product-btns                                         
                    a(href="https://www.youtube.com/watch?v=mrg4mudwA4M").btn.btn-large.waves-effect.waves-light.red.white-text Assembly Instructions    
                    a(href="/pdf/liberty_small_ai.pdf",target="_blank").btn.btn-large.waves-effect.waves-light.red.white-text Download PDF                                    

                h3.product-title.center-align Liberty Shoe Rack 
                p.category-description  
                strong Liberty Shoe Rack - 
                | wooden finished shoe rack for houses and commercial establishments
                p.category-description 
                strong Recommended 
                |for Household, Office, Kitchens.
                p.category-description 
                strong Dimensions: 
                |(H) 790 mm (W) 710 mm (D) 440 mm
                .row.center-align
                    .col.s12.m4.offset-m4
                        .card.product-card.z-depth-2
                            .card-image.center-align
                                img(src="images/products/furniture/liberty_shoe_rack.png", alt="Liberty Shoe Rack, Storage Solutions, Shoe Rack, Deccan Plast Industries")
                            .card-content.center-align.pink-card
                                span.card-title Liberty Shoe Rack
                                p.texture-desc (Wooden Finish)
                                p.price MRP: ₹ 4425/-
                                .right-align
                                        a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Liberty Shoe Rack")
                                            i.material-icons shopping_cart
                .product-btns 
                    a(href="https://www.youtube.com/watch?v=_G3246f5YKw").btn.btn-large.waves-effect.waves-light.red.white-text Assembly Instructions                  
                    a(href="/pdf/liberty_shoe_rack_ai.pdf",target="_blank").btn.btn-large.waves-effect.waves-light.red.white-text Download PDF                           
              
                .center-align                                                        
                    a(href="products.html" class="waves-effect waves-light btn-small red") 
                        i.material-icons.left arrow_back
                        | BACK   



